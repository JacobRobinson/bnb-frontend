  <body>

    <div class="container">

      <form action="add" method="POST">
        <h2 class="form-signin-heading">Add your Property</h2>
        <input type="text" name="name" id="inputName" class="form-control" placeholder="Name" required autofocus>
        <input type="text" name="address" id="inputAddress" class="form-control" placeholder="Address" required>
        <input type="text" name="type" id="inputType" class="form-control" placeholder="Type" required autofocus>
        <input type="text" name="capacity" id="inputCapacity" class="form-control" placeholder="Occupant Capacity" required autofocus>
        <input type="text" name="beds" id="inputBeds" class="form-control" placeholder="Number of Beds" required autofocus>
        <input type="text" name="price" id="inputPrice" class="form-control" placeholder="Price in CAD" required autofocus>
        <input type="text" name="image_url" id="inputImageUrl" class="form-control" placeholder="Image URL" required autofocus>
        </div>
        <input class="btn btn-lg btn-primary btn-block" type="submit" value="Add"></input>
      </form>

    </div> <!-- /container -->


